  "description": "Starter site for a Vital Design System Bodiless site using NextJS",
  "author": "Chris Oden <coden@its.jnj.com>",
  "homepage": "",
  "license": "Apache-2.0",
  "private": true,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "dependencies": {
    "--vital-next--": "^1.0.0-rc.37",
    "@bodiless/backend": "^1.0.0-rc.37",
    "@bodiless/cli": "^1.0.0-rc.37",
    "@bodiless/components": "^1.0.0-rc.37",
    "@bodiless/core": "^1.0.0-rc.37",
    "@bodiless/documentation": "^1.0.0-rc.37",
    "@bodiless/fclasses": "^1.0.0-rc.37",
    "@bodiless/search": "^1.0.0-rc.37",
    "@bodiless/next": "^1.0.0-rc.37",
    "@bodiless/ui": "^1.0.0-rc.37",
    "@bodiless/vital-buttons": "^1.0.0-rc.37",
    "@bodiless/vital-editors": "^1.0.0-rc.37",
    "@bodiless/vital-elements": "^1.0.0-rc.37",
    "@bodiless/vital-flowcontainer": "^1.0.0-rc.37",
    "@bodiless/vital-image": "^1.0.0-rc.37",
    "@bodiless/vital-layout": "^1.0.0-rc.37",
    "@bodiless/vital-link": "^1.0.0-rc.37",
    "@bodiless/vital-list": "^1.0.0-rc.37",
    "@bodiless/vital-navigation": "^1.0.0-rc.37",
    "@bodiless/vital-table": "^1.0.0-rc.37",
    "@bodiless/vital-templates": "^1.0.0-rc.37",
    "@bodiless/webpack": "^1.0.0-rc.37",
    "autoprefixer": "^10.4.2",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "lodash": "^4.17.19",
    "mobx-react": "^7.2.1",
    "next": "^13.3.4",
    "ora": "^4.0.2",
    "postcss": "^8.4.12",
    "rc-tooltip": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "scripts": {
    "build": "npm-run-all build:env-vars build:lib build:search",
    "build:docs": "bl-docs-build",
    "build:env-vars": "bodiless generate-env-vars",
    "build:lib": "next build",
    "build:search": "create-search-index",
    "build:stats": "BODILESS_BUILD_STATS=1 npm run build",
    "build:edit": "npm run build",
    "check": "tsc -p ./tsconfig.check.json",
    "clean": "rimraf .next next-env.d.ts",
    "dev-backend": "BL_IS_EDIT=1 bodiless-backend",
    "dev-frontend": "node --stack-size=3200 ./node_modules/.bin/next dev -p ${APP_PORT:-8000}",
    "edit-frontend": "run-s build:edit serve:edit",
    "lint": "next lint",
    "docs": "run-s build:docs serve:docs",
    "prepare": "node ../../scripts/set-missing-symlinks.js @bodiless",
    "prestart": "run-s build:env-vars",
    "search-index": "create-search-index",
    "serve": "next start -p ${APP_PORT:-9000}",
    "serve:docs": "docsify serve ./public/generated/doc",
    "serve:edit": "next start -p ${APP_PORT:-8000}",
    "start": "run-p -r edit-frontend dev-backend",
    "dev": "run-p -r dev-frontend"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@statoscope/webpack-plugin": "^5.20.1",
    "cross-env": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.23",
    "tailwindcss-dir": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^4.0.0",
    "@next/eslint-plugin-next": "^12.3.4"
  }
}
